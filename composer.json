{
    "name": "ndradev/okeconnect-php-client",
    "description": "PHP Client untuk OkeConnect H2H API - Parsing lengkap untuk transaksi, webhook, dan price list",
    "version": "4.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": ["okeconnect", "ppob", "h2h", "pulsa", "api", "php", "indonesia"],
    "authors": [
        {
            "name": "ndradev",
            "email": "cindramicin@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "OkeConnect\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "OkeConnect\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
